    "@storybook/addons": "^5.3.12",
    "@storybook/preact": "^5.3.12",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-preact": "^1.1.6",
